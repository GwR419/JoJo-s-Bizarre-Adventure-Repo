    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>JoJo's Bizarre Adventure Part 5: Golden Wind</title>
            </titleStmt>
            <publicationStmt>
                <p>Publication Information</p>
            </publicationStmt>
            <sourceDesc>
                <p>Information about the source</p>
            </sourceDesc>
        </fileDesc>
